<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
                  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.skhu.NWFA.user.userDao.userDaoS">

	<select id="selectID"
		resultType="com.skhu.NWFA.user.userModel.userModel">
		select * from User where
		login_id = #{userID}
	</select>

	<insert id="insertInfo">
		insert into SejongStage
		(User_user_id,Sejong_Word_word_id,star,open_game)
		values(#{user_id},1,0,1)
	</insert>
	
	<insert id="insertInfo2">
		insert into SejongStage
		(User_user_id,Sejong_Word_word_id,star,open_game)
		values(#{user_id},#{i},0,0)
	</insert>
	
	<insert id="insertInfo3">
		insert into SejongStage
		(User_user_id,Sejong_Word_word_id,star,open_game)
		values(#{user_id},#{i},0,0)
	</insert>
</mapper>